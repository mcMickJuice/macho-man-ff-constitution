    "concurrently": "^3.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.14.0",
    "express-http-proxy": "^1.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "npm-check": "^5.4.0",
    "pre-commit": "^1.2.2",
    "style-loader": "^0.18.1",
    "tape": "^4.6.3",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
