// Place your settings in this file to overwrite default and user settings.
{
    "vsicons.presets.angular": false,
    "eslint.autoFixOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/_site": true
    },
    "eslint.enable": true,
    "prettier.singleQuote": true,
    "editor.formatOnSave": true,
    "[html]": {
        "editor.formatOnSave": false
    }
}